<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane fx:id="anchorPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="map7Client.MainController">
   <children>
      <Label fx:id="log_lbl" layoutX="20.0" layoutY="214.0" text="&lt;&gt;" textFill="#00000076" />
      <Pane id="visualizzatore" prefHeight="270.0" prefWidth="524.0" visible="false">
         <children>
            <TextArea layoutX="11.0" layoutY="11.0" prefHeight="248.0" prefWidth="502.0" visible="false" />
         </children>
      </Pane>
      <MenuBar prefHeight="25.0" prefWidth="524.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <menus>
          <Menu id="help" mnemonicParsing="false" text="Help">
            <items>
              <MenuItem id="log" fx:id="logMenuItem" mnemonicParsing="false" onAction="#showLogDialog" onMenuValidation="#showLogDialog" text="Log" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Label layoutX="20.0" layoutY="231.0" text="Powered by Gaetano Dibenedetto" />
      <Button id="process_btn" fx:id="processBtn" layoutX="312.0" layoutY="110.0" mnemonicParsing="false" onMouseClicked="#process" text="Process" AnchorPane.bottomAnchor="135.0" AnchorPane.leftAnchor="312.0" AnchorPane.rightAnchor="156.0" AnchorPane.topAnchor="110.0" />
      <TextField id="input_txt_filename" fx:id="input_txt_filename" disable="true" editable="false" layoutX="43.0" layoutY="110.0" prefHeight="25.0" prefWidth="158.0" promptText="File name (with extension)" AnchorPane.bottomAnchor="135.0" AnchorPane.leftAnchor="43.0" AnchorPane.rightAnchor="323.0" AnchorPane.topAnchor="110.0" />
      <Group layoutX="43.0" layoutY="56.0">
         <children>
            <RadioButton id="radio_btn_learn" fx:id="rblearn" mnemonicParsing="false" text="Learn Regression Tree from data">
               <toggleGroup>
                  <ToggleGroup fx:id="radiogroup" />
               </toggleGroup>
            </RadioButton>
            <RadioButton id="radio_btn_load" fx:id="rbload" layoutY="17.0" mnemonicParsing="false" text="Load Regression Tree from archive" toggleGroup="$radiogroup" />
         </children>
      </Group>
   </children>
</AnchorPane>
